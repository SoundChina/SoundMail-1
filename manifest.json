{
  "name": "SoundMail",
  "version": "0.2",
  "description": "Add sounds to your gmail!",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.11.3.min.js",
    "gmail.js",
    "main.js"
  ],
  "manifest_version": 2
}
